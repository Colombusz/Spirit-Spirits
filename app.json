{
  "expo": {
    "name": "spiritspirits",
    "slug": "spiritspirits",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ghost.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ghost.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "BACKEND_URL": "http://192.168.0.155:5000",
      "eas": {
        "projectId": "b577b503-6578-436e-bfe3-bc3623f1acc5"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.spiritspirits.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ghost.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/ghost.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.974909794033-6p26ghnf0n58jopi6dl7mc3nrd4k7n2u"
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          }
        }
      ],
      [
        "expo-build-properties",
        {}
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "owner": "reimuhakurei"
  }
}
