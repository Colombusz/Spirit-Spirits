{
  "expo": {
    "name": "spiritspirits",
    "slug": "spiritspirits",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ghost.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ghost.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "BACKEND_URL": "http://192.168.100.12:5000",
      "eas": {
        "projectId": "b577b503-6578-436e-bfe3-bc3623f1acc5"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.spiritspirits.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ghost.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/ghost.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          }
        }
      ],
      "expo-build-properties"
    ],
    "owner": "reimuhakurei"
  }
}
