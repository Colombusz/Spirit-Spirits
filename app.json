{
  "expo": {
    "name": "spiritspirits",
    "slug": "spiritspirits",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ghost.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ghost.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "BACKEND_URL": "http://192.168.100.12:5000",
      "eas": {
        "projectId": "b577b503-6578-436e-bfe3-bc3623f1acc5"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.spiritspirits.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ghost.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "bundleIdentifier": "com.spiritspirits.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "web": {
      "favicon": "./assets/ghost.png"
    },
    "plugins": [
      [
      "@react-native-google-signin/google-signin",
      {
        "googleServicesFile": "./google-services.json",
        "iosUrlScheme": "com.googleusercontent.apps.974909794033-6p26ghnf0n58jopi6dl7mc3nrd4k7n2u"
      }
      ],
      [
      "expo-dev-client",
      {
        "launchMode": "most-recent"
      }
      ],
      
      [
      "expo-sqlite",
      {
        "enableFTS": true,
        "useSQLCipher": true,
        "android": {
        "enableFTS": false,
        "useSQLCipher": false
        }
      }
      ],
      [
      "expo-build-properties",
      {
        "ios": {
        "useFrameworks": "static"
        }
      }
      ],
      "@react-native-async-storage/async-storage"
    ],
    "owner": "reimuhakurei"
  }
}